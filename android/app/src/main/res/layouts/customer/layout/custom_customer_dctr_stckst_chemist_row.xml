            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="5dp"
                android:layout_marginRight="20dp"
                android:layout_marginLeft="17dp"
                android:textSize="17sp"
                android:textColor="#616161"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:text="Dr. Abcd"/>
            <TextView
                android:id="@+id/tv_speciality"
                android:layout_below="@id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:layout_marginLeft="17dp"
                android:visibility="visible"
                android:textSize="17sp"
                android:textColor="#a7a7a7"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:text="Orthopedic Surgeon"/>
            <TextView
                android:id="@+id/tv_work_place"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_speciality"
                android:layout_marginLeft="17dp"
                android:textSize="15sp"
                android:textColor="#616161"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:text="Park Street Area"/>
            <TextView
                android:id="@+id/tv_customer_class"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_work_place"
                android:layout_marginLeft="17dp"
                android:paddingBottom="8dp"
                android:visibility="visible"
                android:textSize="13sp"
                android:textColor="#9C27B0"
                android:clickable="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:text="Gold"/>
            <LinearLayout
                android:id="@+id/ll_border"
                android:layout_below="@id/rl_items"
                android:layout_width="match_parent"
                android:layout_marginTop="0dp"
                android:layout_height="1dp"
                android:orientation="vertical"
                android:background="#dcdcdc"/>


